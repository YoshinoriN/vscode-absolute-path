{
  "name": "display-absolute-path",
  "displayName": "display-absolute-path",
  "description": "",
  "version": "0.0.1",
  "publisher": "YoshinoriN",
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Absolute path extention configurations",
      "properties": {
        "absolutepath.defaultPathStyle": {
          "type": "string",
          "default": "unix",
          "enum": [
            "unix",
            "windows"
          ],
          "description": "Specify default path style. Unix like or Windows like.",
          "scope": "window"
        }
      }
    },
    "commands": [
      {
        "command": "absolutepath.showQuickPicker",
        "title": "Absolute Path: Open Menus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.62",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "typings": "^2.1.1",
    "vscode": "^1.1.16"
  }
}
